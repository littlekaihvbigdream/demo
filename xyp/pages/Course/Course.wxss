/* 城市选择 */

.city {
  padding: 20rpx 0;
  width: 100%;
  height: 400rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  text-align: center;
}

.city>view::-webkit-scrollbar {
  display: none;
}

.city>view {
  width: 100%;
  height: 100%;
  color: #999;
  font-size: 28rpx;
  overflow: scroll;
}

.city image {
  width: 10rpx;
  height: 20rpx;
  margin-left: 10rpx;
}

.cityName {
  margin: 10rpx 0;
}

.cityActives {
  color: #3bc26d;
}
