
page{
  width: 100%;
  height: 100%;
}

.name {
  display: flex;
  justify-content: space-around;
  font-size: 32rpx;
  color: #434343;
  padding: 40rpx 0;
  background-color: #fff;
  border-bottom: 1px solid #e1e1e1;
}

.name>view {
  display: inline-block;
  text-align: center;
  flex: 1;
}

.none-data {
  width: 100%;
  height: 100%;
  display: flex;
  align-content: center;
  align-items: center;
  justify-content: center;
}