page {
  background-color: #f6f5f5;
}

.labels {
  padding: 30rpx;
  background-color: #fff;
}

.b-titel {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.b-titel>view {
  color: #434343;
  font-size: 30rpx;
}

.choosed {
  background-color: #3bc26d !important;
  color: #fff !important;
}

.labelBox {
  padding-top: 30rpx;
}

.labelBox>view {
  width: 150rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #3bc26d;
  color: #fff;
  font-size: 28rpx;
  display: inline-block;
  border-radius: 30rpx;
  margin: 10rpx;
}

.slectLabel {
  padding: 30rpx;
  background-color: #fff;
}

.comLabel {
  margin-top: 20rpx;
}

.labelBtn {
  display: inline-block;
  /* padding: 10rpx 20rpx; */
  width: 150rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #e2e2e2;
  color: #3bc26d;
  font-size: 28rpx;
  border-radius: 30rpx;
  margin-top: 30rpx;
  position: relative;
  margin: 10rpx 10rpx;
}

.labelBtnActive {
  background-color: #3bc26d;
  color: #fff;
}

.labelBtn>icon {
  position: absolute;
  right: -15rpx;
  top: -5rpx;
}

.adds {
  width: 150rpx;
  height: 60rpx;
  color: #3bc26d;
  font-size: 28rpx;
  border-radius: 30rpx;
  display: inline-block;
  margin: 10rpx 10rpx;
  border: 1px dashed #aeabab;
  text-align: center;
  line-height: 60rpx;
}

.popup {
  position: fixed;
  top: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.3);
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.popupBox {
  width: 550rpx;
  height: 400rpx;
  background-color: #fff;
  border-radius: 30rpx;
  padding: 30rpx 0;
}

.content {
  text-align: center;
  border-bottom: 1px solid #d8d8d8;
  padding: 0 30rpx;
}

.content>view:nth-child(1) {
  margin-top: 30rpx;
  font-size: 34rpx;
  color: #434343;
}

.content>view:nth-child(2) {
  font-size: 28rpx;
  color: #999;
  margin-top: 30rpx;
}

.content>input {
  padding-left: 10rpx;
  border: 1px solid #cdcaca;
  text-align: left;
  height: 75rpx;
  color: #999;
  font-size: 28rpx;
  margin-top: 30rpx;
  border-radius: 15rpx;
  margin-bottom: 80rpx;
}

.popupBotm {
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.popupBotm>view {
  width: 100%;
  color: #3bc26d;
  font-size: 34rpx;
  text-align: center;
}

.popupBotm>.bor {
  height: 90rpx;
  width: 1px;
  background-color: #d8d8d8;
}

.all{
  padding: 0.5rem;
  color: #5c5c5c;
  margin-top: 20rpx;
  font-size: 30rpx;
  background-color: #fff;
}
.system,.grade{
  background-color: #fff;
  padding-left: 0.5rem;
  padding-top: 0.5rem;
}
.box{
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
}