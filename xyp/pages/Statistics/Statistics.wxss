page {
  background-color: #f6f5f5;
  margin-bottom: 166rpx;
}

.navBox {
  position: fixed;
  top: 0;
  width: 100%;
  z-index: 999;
}

.main {
  margin-top: 420rpx;
}

.top {
  padding: 40rpx 30rpx;
}

.nav {
  height: 70rpx;
  border: 1px solid #fff;
  border-radius: 45rpx;
  display: flex;
  overflow: hidden;
}

.nav>view {
  width: 100%;
  height: 100%;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
  color: #fff;
}

.nav>view:nth-child(1) {
  border-right: 1px solid #3bc26d;
}

.navActive {
  background-color: #fff;
  color: #5ed606 !important;
}

.navActive1 {
  background-color: #fff;
  color: #41c471 !important;
}

.showNum {
  padding: 40rpx;
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}

.showNums {
  width: 100%;
  text-align: center;
  display: flex;
  justify-content: center;
}

.showNums>image {
  width: 75rpx;
  height: 75rpx;
}

.showNums>view {
  font-size: 32rpx;
  color: #fff;
}

.num1 {
  margin-left: 10rpx;
  color: #fff !important;
  font-weight: bold;
}

.num2 {
  color: #ffc500 !important;
  font-weight: bold;
}

.b_down {
  height: 100rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #534f4f;
  border-bottom: 1px solid #f6f5f5;
}

.b_down>view {
  width: 100%;
  line-height: 85rpx;
  font-size: 32rpx;
  height: 80rpx;
  text-align: center;
}

.b_down>view>image {
  width: 35rpx;
  height: 40rpx;
}

.txt {
  display: flex;
  align-items: center;
  justify-content: center;
}

.tabActive1 {
  color: #3bc26d !important;
}

.commodity {
  margin-top: 20rpx;
  background-color: #fff;
}

.commodityList {
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding: 10rpx 30rpx;
  border-bottom: 1px solid #f6f5f5;
}

.commodityList>image {
  width: 300rpx;
  height: 130rpx;
  margin-right: 10rpx;
}

.commodityList>view {
  width: 100%;
}

.comm-text image {
  width: 18rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.comm-text>view:nth-child(1) {
  font-size: 30rpx;
  color: #534f4f;
}

.comm-text>view:nth-child(3) {
  font-size: 28rpx;
  color: #3bc26d;
  margin-top: 10rpx;
  margin-right: 30rpx;
}

.b-comm {
  color: #999;
  font-size: 26rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.b-comm>text {
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.down {
  color: #999;
  font-size: 28rpx;
  padding: 30rpx;
  text-align: center;
}

/* 顾客统计 */

.customer {
  margin-top: 20rpx;
  height: 250rpx;
  background-color: #fff;
}

.customer-t {
  position: relative;
  margin-top: 30rpx;
  padding: 20rpx 0;
  height: 100rpx;
  display: flex;
  align-items: center;
  text-align: center;
}

.customer-t>view {
  width: 100%;
}

.customer-t>view>view {
  color: #fff;
  font-size: 32rpx;
}

.customer-t>view>view:nth-child(2) {
  font-weight: bold;
}

.customer-t>view:nth-child(1) {
  border-right: 1px solid #fff;
}

.customer-t image {
  width: 75rpx;
  height: 75rpx;
}

.customer-t text {
  margin-left: 10rpx;
  color: #534f4f;
  font-size: 30rpx;
  font-weight: bold;
}

.customer-b {
  box-shadow: 2px 2px 10px #c5c5c5;
  /* box-shadow: 0px 3px 7px 0px rgba(0, 0, 0, 0.08); */
  background-color: #fff;
  position: absolute;
  bottom: -85%;
  z-index: 9999;
  padding: 30rpx 0;
  display: flex;
  align-items: center;
  text-align: center;
  border-radius: 30rpx;
}

.customer-b image {
  width: 75rpx;
  height: 75rpx;
}

.box {
  height: 100%;
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.box:nth-child(2) {
  border-right: 1px solid #f6f5f5;
  border-left: 1px solid #f6f5f5;
}

.box>view>view:nth-child(1) {
  font-size: 28rpx;
  color: #999;
  font-weight: 400;
}

.box>view {
  margin-left: 10rpx;
}

.box>view>view:nth-child(2) {
  font-size: 30rpx;
  color: #534f4f;
  font-weight: 400;
}

.times {
  padding: 0 30rpx;
  margin-top: 100rpx;
  height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}

.times image {
  width: 40rpx;
  height: 35rpx;
  margin: 0 10rpx;
}

.times .foot-img {
  width: 45rpx;
  height: 45rpx;
  margin-left: 0;
}

.times>view:nth-child(1) {
  display: flex;
  align-items: center;
}

.times-sle>text {
  font-size: 30rpx;
  color: #534f4f;
}

.customerList {
  background-color: #fff;
}

.customerList-li {
  padding: 20rpx 30rpx;
  padding-left: 100rpx;
  padding-bottom: 80rpx;
  position: relative;
  border-bottom: 1px solid #f6f5f5;
}

.customerList-li>image:nth-child(1) {
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.customerList-li>image:nth-child(2) {
  position: absolute;
  top: 60rpx;
  left: 60rpx;
  width: 45rpx;
  height: 45rpx;
}

.customerList-name {
  display: flex;
  justify-content: space-between;
}

.customerList-name>image {
  width: 18rpx;
  height: 30rpx;
}

.customerList-name>.name>text {
  font-size: 30rpx;
  color: #434343;
}

.customerList-name>.name>image {
  width: 35rpx;
  height: 35rpx;
  margin: 0 10rpx;
}

.customerList-name>.name {
  display: flex;
  align-items: center;
}

.labels>view {
  padding: 0 0.3rem;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 26rpx;
  background-color: #ffbc3a;
  color: #fff;
  margin-left: 10rpx;
  display: inline-block;
}

.txtList {
  color: #999;
  font-size: 28rpx;
  padding-top: 10rpx;
}

.label-f {
  float: right;
  display: flex;
  height: 80rpx;
}

.label-f>view {
  margin-left: 20rpx;
  width: 150rpx;
  height: 55rpx;
  border: 1px solid #3bc26d;
  border-radius: 25rpx;
  color: #3bc26d;
  font-size: 28rpx;
  text-align: center;
  line-height: 55rpx;
}

.label-f>button {
  margin-left: 20rpx;
  width: 185rpx;
  height: 60rpx;
  border: 1px solid #3bc26d;
  border-radius: 25rpx;
  color: #3bc26d;
  font-size: 28rpx;
  text-align: center;
  line-height: 55rpx;
}

/* 下拉样式 */

.b-slect {
  /* width: 60rpx; */
  background-color: #3bc26d;
  text-align: center;
  position: absolute;
  bottom: -200rpx;
  left: 70rpx;
  z-index: 998;
  border-radius: 10rpx;
}

.Arrow {
  position: absolute;
  top: -1.2rem;
  width: 0;
  height: 0;
  border: 28rpx solid;
  border-color: transparent transparent #3bc26d;
  left: 35%;
}
.Arrow1 {
  position: absolute;
  top: -1.2rem;
  width: 0;
  height: 0;
  border: 25rpx solid;
  border-color: transparent transparent #3bc26d;
  left: 35%;
}

.b-slect .tabDays {
  padding: 15rpx 40rpx;
  font-size: 30rpx;
  /* color: #534f4f; */
  color: #fff;
}
.tabActive{
  padding: 15rpx 40rpx;
  font-size: 30rpx;
  /* color: #534f4f; */
  color: #fff;
}

.slect-box {
  padding: 20rpx 0rpx;
  background-color: #3bc26d;
  width: 200rpx;
  border-radius: 15rpx;
  margin-left: 80rpx;
  position: relative;
}

.slect-box .days {
  min-width: 130rpx;
  padding: 5rpx 10rpx;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 60rpx;
  margin: auto;
}

.slect-btnActive {
  color: #fff !important;
  padding: 5rpx 10rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 60rpx;
}

.home-pre-image-container {
  width: 100%;
  height:100%
}

.home-pre-image {
  width: 100%!important;
}