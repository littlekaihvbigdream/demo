/* 下拉城市选择 */

.slectCity {
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
}

.cityBox {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 20rem;
  background-color: #fff;
  padding-bottom: 1rem;
}

.close {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 0.5rem;
  height: 3rem;
  border-bottom: 1px solid #d8d8d8;
  font-size: 32rpx;
}
.close>view:nth-child(2){
  color: #3bc26d;
}
.box {
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 0.5rem;
  overflow: hidden;
}

.box>view {
  overflow-y: scroll;
  width: 100%;
  text-align: center;
  height: 17rem;
}
.box>view>view{
  font-size:30rpx;
  color: #999;
  padding: 0.3rem 0;
}
.box>view::-webkit-scrollbar {
  display: none;
}
.provinceActive{
  color: #3bc26d !important;
}
.txt{
  width:77%;
text-align:right;
}
.boxs{
  overflow: scroll;
  width: 100%;
  height: 17rem;
  font-size: 30rpx;
  text-align: center;
   color: #999;
  padding: 0.3rem 0;
}
.boxs::-webkit-scrollbar {
  display: none;
}
.boxs>view{
  margin: 0.5rem 0;
}