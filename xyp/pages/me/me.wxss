page {
  background-color: #f6f5f5;
  margin-bottom: 166rpx;
}

.userHead {
  width: 100%;
  height: 400rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  text-align: center;
  padding: 30rpx 0;
  background-color: #fff;
}

.headImg {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
}

.userName {
  height: 80rpx;
  font-size: 34rpx;
  color: #fff;
}

.grade {
  width: 120rpx;
  height: 48rpx;
  background-color: #fff;
  color: #4bc678;
  font-size: 28rpx;
  margin: auto;
  line-height: 48rpx;
  border-radius: 25rpx;
  margin-top: 20rpx;
}

.meList {
  background-color: #fff;
  width: 100%;
}

.userList {
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx 30rpx;
  padding-left: 70rpx;
  border-bottom: 1px solid #f6f5f5;
}

.userList>image:nth-child(1) {
  width: 36rpx;
  height: 36rpx;
  position: absolute;
  left: 20rpx;
}

.userList>text {
  font-size: 32rpx;
  color: #434343;
}

.userList>image:nth-child(3) {
  width: 18rpx;
  height: 30rpx;
}

.arrow_right {
  width: 18rpx !important;
  height: 30rpx !important;
}

.text_right {
  margin-left: 460rpx;
}

.quit-button {
  margin: 30rpx 30rpx 30rpx 30rpx;
  background-color: #3bc26d;
  color: #fff;
}

.home-pre-image-container {
  width: 100%;
  height: 100%;
}

.home-pre-image {
  width: 100% !important;
}
