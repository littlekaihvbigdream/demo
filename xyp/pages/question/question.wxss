page{
  background-color: #f6f5f5;
}
.search{
  height: 120rpx;
  background-color: #fff;
  /* padding: 30rpx 30rpx 0 30rpx; */
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  width: 100%;
}
.inp{
  width: 85%;
  border-radius: 40rpx;
  background-color: #f6f5f5;
  padding-left: 60rpx;
  position: relative;
  height: 75rpx;
}
.inp>image{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left: 15rpx;
  top: 20rpx;
}
.inp>input{
  width: 100%;
  height: 100%;
  font-size: 28rpx;
}
.b-titel{
  margin-top: 120rpx;
  height: 90rpx;
  line-height: 90rpx;
  padding:0 30rpx;
  display: flex;
  align-items: center;
}
.b-titel>image{
  width: 47rpx;
  height: 47rpx;
  margin-right: 10rpx;
}
.b-titel>text{
  font-size: 30rpx;
  color: #534F4F;
}
.swiperList{
  height: 655rpx;
  background-color: #fff;
  padding: 30rpx;
}
.b-li{
  width: 33.3%;
  /* height: 200rpx; */
  padding: 45rpx 0;
  text-align: center;
  display: inline-block;
}
.b-li>image{
  width: 80rpx;
  height: 80rpx;
}
.b-li>view{
  font-size: 30rpx;
  color: #534F4F;
  text-align: center;
}
.b-cont{
  background-color: #fff;
  /* height: 220rpx; */
  padding: 30rpx;
  border-bottom: 1px solid #f6f5f5;
}
.b-cont>view:nth-child(1){
  width: 100%;
  color: #534F4F;
  font-size: 30rpx;
}
.b-cont>view:nth-child(2){
  width: 650rpx;
  white-space: nowrap;  
 text-overflow:ellipsis; 
 overflow:hidden;
  color: #999999;
  font-size: 30rpx;
}