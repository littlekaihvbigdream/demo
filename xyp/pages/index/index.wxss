page{
  width: 100%;
  height: 100%;
  background-color: #f6f5f5;
  margin-bottom: 166rpx;
}

.banner{
  width: 100%;
  height: 350rpx;
}
.banner>swiper{
  width: 100%;
  height: 100%;
}
.slide-image{
  width: 100%;
  height: 100%;
}
.b-nav{
  padding: 50rpx 20rpx;
  background-color: #fff;
  margin-bottom: 120rpx;
}
.ProductTutorial{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ProductTutorial>image:nth-child(1){
  width: 460rpx;
  height: 270rpx;
}
.ProductTutorial>image:nth-child(2){
  width: 222rpx;
  height: 270rpx;
}
.Train{
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.Train>image:nth-child(1){
   width: 222rpx;
  height: 270rpx;
}
.Train>image:nth-child(2){
  width: 460rpx;
  height: 270rpx;
}
.resources{
  width: 100%;
  height: 280rpx;
  display: flex;
  align-items: center;
}
.txt{
  margin-left: 30rpx;
}
.txt>view{
  color: #fff;
  font-size: 30rpx;
}
.txt>view:nth-child(1){
  font-size: 45rpx;
}
.resources1{
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
}
.Trains{
  width: 48.5%;
  border-radius: 15rpx;
  height: 180rpx;
  display: flex;
  align-items: center;
}
.Trains>view{
  margin-left: 30rpx;
  color: #fff;
  font-size: 35rpx;
}
.resources:nth-child(1){
  margin-bottom: 30rpx;
}
.resources:nth-child(2){
  height: 165rpx;
  border-radius: 75rpx;
}

.home-pre-image-container {
  width: 100%;
  height:100%
}

.home-pre-image {
  width: 100%!important;
}