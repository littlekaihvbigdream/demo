/* pages/trainingRecord/trainingRecord.wxss */
page {
  width: 100%;
  height: 100%;
  background-color: #f6f5f5;
}

.screen {
  padding-top: 48rpx;
  background-color: #3bc26d;
}

.screenTime {
  display: flex;
  align-items: top;
  justify-content: space-between;
  padding: 0rpx 64rpx 0rpx 64rpx;
  padding-bottom: 0;
}

.increase-rate-zero {
  color: #fc0;
}

.increase-rate-negative {
  color: #ea5a5a;
}

.increase-rate-positive {
  color: #3bc26d;
}

.picker {
  color: #fff;
  font-size: 30rpx;
}

.timeType>view {
  color: #fff;
  font-size: 30rpx;
  text-align: center;
}

.timeType {
  text-align: center;
}

.timeType>image {
  width: 45rpx;
  height: 40rpx;
}

.opt_box {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.opt {
  padding-left: 24rpx;
  padding-bottom: 24rpx;
  position: relative;
}

.cell-disclosure {
  width: 20rpx;
  height: 30rpx;
  /* background: red; *//* position: absolute; */
}

.showArrow {
  position: absolute;
  bottom: 0;
  left: 45%;
  width: 0;
  height: 0;
  border: 12rpx solid;
  z-index: 10;
  border-color: transparent transparent #fff transparent;
}

.showArrow1 {
  position: absolute;
  bottom: 0;
  left: 30%;
  width: 0;
  height: 0;
  border: 12rpx solid;
  z-index: 10;
  border-color: transparent transparent #fff transparent;
}

.openRate {
  position: absolute;
  bottom: 0;
  left: 20%;
  width: 0;
  height: 0;
  border: 12rpx solid;
  z-index: 10;
  border-color: transparent transparent #fff transparent;
}

.open_rate {
  box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.3);
  position: absolute;
  top: 75rpx;
  right: 24rpx;
  width: 80%;
  background-color: #fff;
  padding: 20rpx;
}

.open_rate>view {
  text-align: center;
  padding: 20rpx 0;
  font-size: 28rpx;
  color: #666;
}

.open_rate>view:nth-child(1) {
  border-bottom: 1px solid #dcdcdc;
}

.open_rate>view:nth-child(2) {
  border-bottom: 1px solid #dcdcdc;
}

.show_forward {
  box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.3);
  position: absolute;
  top: 75rpx;
  left: 24rpx;
  width: 80%;
  background-color: #fff;
  padding: 20rpx;
}

.show_forward>view {
  text-align: center;
  padding: 20rpx 0;
  font-size: 28rpx;
  color: #666;
}

.show_forward>view:nth-child(1) {
  border-bottom: 1px solid #dcdcdc;
}

.show_forward>view:nth-child(2) {
  border-bottom: 1px solid #dcdcdc;
}

.show_Statistics {
  box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.3);
