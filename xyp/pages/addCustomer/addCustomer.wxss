page {
  background-color: #f6f5f5;
}

.b_down {
  position: fixed;
  top: 0;
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #534f4f;
  z-index: 10;
}

.b_down>view {
  /* width: 85rpx; */
  line-height: 85rpx;
  height: 80rpx;
  font-size: 32rpx;
  text-align: center;
  width: 100%;
}
.labels>view {
  padding: 0 0.3rem;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 26rpx;
  background-color: #ffbc3a;
  color: #fff;
  margin-left: 10rpx;
  display: inline-block;
}
.tabActive {
  color: #3bc26d;
  border-bottom: 5rpx solid #3bc26d;
}

.b_downs {
  margin: auto;
  margin-top: 30rpx;
  width: 70%;
  background-color: #3bc26d;
  border-radius: 20rpx;
  position: relative;
}

.horn {
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  top: -10rpx;
  right: 45%;
  background-color: #3bc26d;
  transform: rotate(45deg);
  z-index: -1;
}

.b_downs>view {
  color: #fff;
  border-bottom: 1px solid #59d687;
}

.b_downs>view:last-child {
  border-bottom: none;
}

.icon-down {
  width: 35rpx;
  height: 35rpx;
  vertical-align: middle;
}


.customerList {
  margin-top: 120rpx;
  background-color: #fff;
}

.customerList-name>.name>image {
  width: 35rpx;
  height: 35rpx;
  margin: 0 10rpx;
}

.customerList-li {
  padding: 20rpx 30rpx;
  padding-left: 100rpx;
  padding-bottom: 80rpx;
  position: relative;
  border-bottom: 1px solid #f6f5f5;
}

.customerList-li>image {
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.customerList-name {
  display: flex;
  justify-content: space-between;
}

.customerList-name>image {
  width: 18rpx;
  height: 30rpx;
}

.customerList-name>.name>text {
  font-size: 30rpx;
  color: #434343;
}

.customerList-name>.name {
  display: flex;
  align-items: center;
}

.customerList-name>.name>view>view {
  width: 110rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 26rpx;
  background-color: #ffbc3a;
  color: #fff;
  display: inline-block;
  margin-left: 10rpx;
}

.txtList {
  color: #999;
  font-size: 28rpx;
  padding-top: 10rpx;
}

.label-f {
  float: right;
  display: flex;
  height: 80rpx;
}

.label-f>button {
  margin-left: 20rpx;
  width: 185rpx;
  height: 60rpx;
  border: 1px solid #3bc26d;
  border-radius: 25rpx;
  color: #3bc26d;
  font-size: 28rpx;
  text-align: center;
  line-height: 55rpx;
}

.label-f>view {
  margin-left: 20rpx;
  width: 150rpx;
  height: 55rpx;
  border: 1px solid #3bc26d;
  border-radius: 25rpx;
  color: #3bc26d;
  font-size: 28rpx;
  text-align: center;
  line-height: 55rpx;
}

.ranking {
  margin-top: 120rpx;
}

.tips {
  margin: 20rpx 0;
  color: #3bc26d;
  font-size: 28rpx;
  background-color: #fff;
  padding: 20rpx 30rpx;
}

.meRank {
  height: 120rpx;
  background-color: #4dc077;
  display: flex;
  align-items: center;
  padding: 20rpx 30rpx;
}

.meRank>view:nth-child(1) {
  width: 70rpx;
  height: 70rpx;
  background-color: #fff;
  border-radius: 50%;
  text-align: center;
  line-height: 70rpx;
  color: #3bc26d;
}

.meRank>view:nth-child(2)>view {
  color: #fff;
  font-size: 30rpx;
  margin-left: 20rpx;
}

.b-list {
  height: 120rpx;
  margin-top: 20rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #f6f5f5;
  padding: 20rpx 30rpx;
}

.rankNum {
  width: 70rpx;
  height: 70rpx;
  background-color: #97f9bc;
  color: #fff;
  border-radius: 50%;
  text-align: center;
  line-height: 70rpx;
}

.listName>view:nth-child(1) {
  color: #434343;
  font-size: 30rpx;
  margin-left: 20rpx;
}
.listName>view:nth-child(2) {
  color: #434343;
  font-size: 30rpx;
  margin-left: 20rpx;
}

.listName>view:nth-child(3) {
  color: #999;
  font-size: 28rpx;
  margin-left: 20rpx;
}

.b-list:nth-child(1)>.rankNum {
  background-color: #ff0018;
}

.b-list:nth-child(2)>.rankNum {
  background-color: #ffc000;
}

.b-list:nth-child(3)>.rankNum {
  background-color: #e9e4e5;
}

.down {
  color: #999;
  font-size: 28rpx;
  padding: 30rpx;
  text-align: center;
}
